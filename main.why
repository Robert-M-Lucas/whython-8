struct MyStruct {
    a: int,
    b: int
}

impl MyStruct {
    fn print(&self) {
        printi(self.a);
        printi(self.b);
    }

    fn increment(&self) {
        self.a += 1;
        self.b += 1;
    }
}

fn main() -> int {
    let x: MyStruct = MyStruct {
        a: 3,
        b: 4
    };

    x.print();
    x.increment();
    x.print();

    return x.a + x.b;
}